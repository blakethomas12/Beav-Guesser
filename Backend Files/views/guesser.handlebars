<main>
  <p>Guesser</p>
  <!-- This is the window for the google street view api -->
  <button onclick="loadRandomStreetView()">New Round</button>
  <div id="street-view-window">
    <div style="width: 100%; overflow: hidden; height: 600px;">
      <iframe
        id="streetViewFrame"
        src="getRandomStreetViewEmbedLink()"
        width="100%"
        height="900"
        frameborder="0"
        style="border:0; margin-top: -150px;"
      >
      </iframe>
    </div>
  </div>

  <!-- This is the window for the OSU map -->
  <div id="guessing-map">
    <img src="..\image_files\campus-map.jpg" alt="Map" id="overlay-map" />
    <canvas id="guess-canvas"></canvas>
  </div>
</main>